{
  "name": "vue-rss-parser",
  "version": "0.0.3",
  "description": "A simple component to parse and embed Medium RSS feeds with Vue.",
  "keywords": [
    "RSS",
    "Embed",
    "RSS Feeds",
    "Medium",
    "Medium Feed",
    "VueJS",
    "News Feed"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/davision/vue-rss-parser"
  },
  "bugs": {
    "url": "https://github.com/davision/vue-rss-parser/issues"
  },
  "private": false,
  "author": {
    "name": "David L."
  },
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve ./src/demo/demo.js",
    "build-demo": "vue-cli-service build ./src/demo/demo.js --dest docs",
    "build": "vue-cli-service build --target lib --name VueRssParser ./src/VueRssParser.vue",
    "lint": "vue-cli-service lint"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^5.2.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "rss-parser": "^3.7.2",
    "vue": "^2.6.10"
  }
}
